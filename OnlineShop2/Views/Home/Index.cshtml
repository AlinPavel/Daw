@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>CheeseShop</h1>
    <p class="lead">
        Magazinul de brânzeturi este cel mai recunoscut din România datorită calității produselor și a prețului avantajos
    </p>

</div>
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
</head>
<body>

    <div class="container">
        <h2></h2>
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/Content/cheese2.jpg" alt="" style="width:100%;">
                </div>

                <div class="item">
                    <img src="~/Content/cheese.jpg" alt="" style="width:100%;">
                </div>

                <div class="item">
                    <img src="~/Content/cheese3.jpg" alt="" style="width:100%;">
                </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

    <div class="w3-container w3-padding-32" id="projects">
        <h3 class="w3-border-bottom w3-border-light-grey w3-padding-16">Cele mai vizionate produse</h3>
    </div>

    <div class="w3-row-padding">
        <div class="w3-col l3 m6 w3-margin-bottom">
            <div class="w3-display-container">
                <div class="w3-display-topleft w3-black w3-padding">Branza din Sibiu</div>
                <img src="https://www.exquis.ro/wp-content/uploads/2017/05/cum-iti-dai-seama-daca-ai-cumparat-branza-falsa-374640-1024x550.jpg" alt="House" style="width:100%">
            </div>
        </div>
        <div class="w3-col l3 m6 w3-margin-bottom">
            <div class="w3-display-container">
                <div class="w3-display-topleft w3-black w3-padding">Cas de la fermele din Brasov</div>
                <img src="https://branzacinstita.ro/wp-content/uploads/2015/10/18_Casdinlaptedevaca2-820x385.jpg" alt="House" style="width:100%">
            </div>
        </div>
        <div class="w3-col l3 m6 w3-margin-bottom">
            <div class="w3-display-container">
                <div class="w3-display-topleft w3-black w3-padding">Lapte Dorna</div>
                <img src="https://deliveryman.ro/2095-large_default/lapte-ladorna-uht-15-grasime-1-litru.jpg" alt="House" style="width:100%">
